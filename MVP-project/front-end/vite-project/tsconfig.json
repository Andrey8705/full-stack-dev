{
    "compilerOptions": {
      "jsx": "react-jsx",
      "allowJs": true,
      "checkJs": false,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "@/*": [
            "./src/*"
        ],
        "@/components/*": [
            "src/components/*"
        ],
        "@/utils/*": [
        "src/utils/*"
    ]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  